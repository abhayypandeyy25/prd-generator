{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "functions": {
    "api/*.py": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/templates/(.*)",
      "dest": "/api/templates.py"
    },
    {
      "src": "/api/templates$",
      "dest": "/api/templates.py"
    },
    {
      "src": "/api/features/(.*)",
      "dest": "/api/features.py"
    },
    {
      "src": "/api/features$",
      "dest": "/api/features.py"
    },
    {
      "src": "/api/context/(.*)",
      "dest": "/api/context.py"
    },
    {
      "src": "/api/questions/(.*)",
      "dest": "/api/questions.py"
    },
    {
      "src": "/api/questions$",
      "dest": "/api/questions.py"
    },
    {
      "src": "/api/prd/(.*)",
      "dest": "/api/prd.py"
    },
    {
      "src": "/api/projects/(.*)",
      "dest": "/api/projects.py"
    },
    {
      "src": "/api/projects$",
      "dest": "/api/projects.py"
    },
    {
      "src": "/api/stakeholder/(.*)",
      "dest": "/api/stakeholder.py"
    },
    {
      "src": "/api/feedback/(.*)",
      "dest": "/api/feedback.py"
    },
    {
      "src": "/api/analytics/(.*)",
      "dest": "/api/analytics.py"
    },
    {
      "src": "/api/share/(.*)",
      "dest": "/api/share.py"
    },
    {
      "src": "/api/comments/(.*)",
      "dest": "/api/comments.py"
    },
    {
      "src": "/api/health$",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/index$",
      "dest": "/api/index.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
